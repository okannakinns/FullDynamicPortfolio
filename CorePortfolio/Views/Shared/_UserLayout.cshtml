<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("~/Areas/User/Views/Default/HeadPartial.cshtml")

<body>
    <div class="container-scroller">
       
        @await Html.PartialAsync("~/Areas/User/Views/Default/NavbarPartial.cshtml")
        
        <div class="container-fluid page-body-wrapper">
            
            @await Html.PartialAsync("~/Areas/User/Views/Default/SidebarPartial.cshtml")
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    
                        @RenderBody()
                        
                   
                </div>

                @await Html.PartialAsync("~/Areas/User/Views/Default/FooterPartial.cshtml")
             
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    @await Html.PartialAsync("~/Areas/User/Views/Default/ScriptsPartial.cshtml")
    @RenderBody()
</body>

</html>
